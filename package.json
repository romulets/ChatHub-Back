{
  "name": "chathub-back",
  "version": "1.0.0",
  "description": "back for chathub",
  "main": "./src/main/app.js",
  "scripts": {
    "start": "node ./src/main/app.js",
    "development": "nodemon --exec babel-node ./src/main/app.js",
    "build": "babel . -d dist --source-maps --copy-files --ignore 'node_modules,dist'",
    "deploy": "npm run-script build && gcloud app deploy ./dist --quiet --version v0"
  },
  "cloud-repo-tools": {
    "requiresKeyFile": true,
    "requiresProjectId": true,
    "test": {
      "app": {
        "args": [
          "./main/app.js"
        ],
        "msg": "Hello, world!"
      }
    }
  },
  "engines": {
    "node": "8.x.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "elasticsearch": "^15.2.0",
    "express": "^4.17.3",
    "mongodb": "^3.1.4",
    "mongoose": "^5.2.14",
    "query-string": "^6.1.0",
    "regenerator-runtime": "^0.12.1",
    "validate-fields": "^5.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@google-cloud/nodejs-repo-tools": "2.2.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^1.18.3"
  }
}
